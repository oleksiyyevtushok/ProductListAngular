{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "pm",
      "camelCase",
    ],
    "component-selector": [
      true,
      "element",
      "pm",
      "kebab-case"
    ],
    "no-inferrable-types": [true, "ignore-params", "ignore-properties"]
  }
}
